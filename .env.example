# Imply Cloud Configuration
# Copy this file to .env and fill in your actual values

# Imply Organization and Project Settings
IMPLY_ORGANIZATION=your-org-name
IMPLY_REGION=us-east-1
IMPLY_CLOUD_PROVIDER=aws
IMPLY_PROJECT_ID=your-project-id

# Authentication (use ONE of these methods)
# Method 1: API Key (recommended)
IMPLY_API_KEY=your-api-key-here

# Method 2: OAuth Access Token (alternative)
# IMPLY_ACCESS_TOKEN=your-access-token-here

# Optional Settings
LOG_LEVEL=INFO
DEFAULT_QUERY_TIMEOUT_MS=30000
MAX_QUERY_LENGTH=10000
